@model SurveyModel
@{
    ViewData["Title"] = "Form";
}

<h2>Vote For Your Favorite National Park!</h2>
<div class="grid-container">
    <div class="grid-item">
        <form method="post">
            <div asp-validation-summary="All" style="text-align:left;"></div>
            <table>
                <tr>
                    <td style="width:250px; text-align:left; font-weight: bold">Favorite National Park</td>
                    <td style="width:250px; text-align:left">
                        <select asp-for="ParkCode">
                            <option value="GCNP">Grand Canyon National Park</option>
                            <option value="GNP">Glacier National Park</option>
                            <option value="GSMNP">Great Smoky Mountain National Park</option>
                            <option value="GTNP">Grand Teton National Park</option>
                            <option value="MRNP">Mount Rainier National Park</option>
                            <option value="RMNP">Rocky Mountain National Park</option>
                            <option value="YNP">Yellowstone National Park</option>
                            <option value="YNP2">Yosemite National Park</option>
                        </select>
                    </td>
                </tr>
                <tr>
                    <td style="width:250px; text-align:left; font-weight: bold">Email</td>
                    <td style="width:250px; text-align:left; font-weight: bold"><input asp-for="Email" /></td>
                </tr>
                <tr>
                    <td style="width:250px; text-align:left; font-weight: bold">State</td>
                    <td style="width:250px; text-align:left; font-weight: bold"><input asp-for="State" /></td>
                </tr>
            </table>
            <table>
                <tr>
                    <td style="width:150px; text-align:left; font-weight: bold; padding-right: 10px;">Activity Level</td>
                    <td style="width:70px; text-align:left">
                        <input type="radio" name="ActivityLevel" value="Inactive" />Inactive
                    </td>
                    <td style="width:90px; text-align:left">
                        <input type="radio" name="ActivityLevel" value="Sedentary" />Sedentary
                    </td>
                    <td style="width:90px; text-align:left">
                        <input type="radio" name="ActivityLevel" value="Active" />Active
                    </td>
                    <td style="width:125px; text-align:left">
                        <input type="radio" name="ActivityLevel" value="Extremely Active" />Extremely Active
                    </td>
                </tr>
                <tr>
                    <td></td>
                    <td></td>
                    <td><input type="submit" value="Submit" /></td>
                    <td></td>
                    <td></td>
                </tr>
            </table>
        </form>
    </div>
    <div class="grid-item">
        <section class="dancingbear">
